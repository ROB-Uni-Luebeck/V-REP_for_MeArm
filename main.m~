% Clear everything and close everything
clear all
close all
clc

% Add path for the API scripts
addpath('API')

% verbindet mit dem roboterarm -> Simmulation muss laufen
VREPconnect;

% hier kann man die gelenkparameter einstellen
q = ones(7,1)*20;
    
VREPtransfer;
    
    pause();
        
    %vrep.simxFinish(-1); 
%end
 
%vrep.delete();            % Destructor
VREPdisconnect;